{
  "name": "ko-website",
  "private": true,
  "description": "Personal website",
  "version": "1.1.0",
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "autoCssProp": true,
      "debugProp": true
    }
  },
  "author": "Kieran Osgood",
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "babel-plugin-macros": "^2.8.0",
    "http-proxy-middleware": "^1.0.4",
    "netlify-lambda": "^1.6.3",
    "next": "latest",
    "node-sass": "^4.13.1",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "pure-react-carousel": "^1.26.0",
    "qs": "^6.9.4",
    "react": "latest",
    "react-compare-image": "^3.0.2",
    "react-dom": "latest",
    "react-github-calendar": "https://github.com/KieranO547/react-github-calendar.git",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^5.7.2",
    "react-query": "^1.5.8",
    "react-spinners": "^0.8.3",
    "react-toggle": "^4.1.1",
    "react-tooltip": "^4.2.7",
    "styled-components": "^5.1.1",
    "twin.macro": "^1.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@types/react": "^17.0.3",
    "gatsby-plugin-root-import": "^2.0.5",
    "prettier": "^1.19.1",
    "purgecss": "^2.1.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "old-build": "netlify build",
    "build-prod": "npx tailwind build ./src/css/main.css -o ./assets/css/main.css && npm run purge && npm run minify",
    "lambda": "netlify-lambda build .netlify/functions",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "old-start": "netlify-lambda build .netlify/functions && netlify dev",
    "deploy": "netlify deploy",
    "deploy-prod": "netlify deploy --prod",
    "minify": "cleancss -o ./assets/css/main.min.css ./assets/css/main.css",
    "purge": "purgecss --css ./assets/css/main.css --content ./index.html --out ./assets/css/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
